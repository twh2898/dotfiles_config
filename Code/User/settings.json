{
    "workbench.colorTheme": "Atom One Dark",
    "editor.fontSize": 12,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.idea": true,
        "**/build, **/devel": true
    },
    "files.autoSave": "afterDelay",
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "vscode-icons",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80, 120
    ],
    "scm.inputFontSize": 11,
    "debug.console.fontSize": 12,
    "terminal.integrated.fontSize": 12,
    "clock.format": "ddd mmm d, yyyy h:mm TT",
    "clock.iconName": "none",
    "clock.alignment": "Right",
    "vtools.autoHideBottomBar": true,
    "git.enableSmartCommit": true,
    "markdownlint.config": {
        "MD034": false,
        "MD013": false,
        "MD012": false,
        "MD040": false,
        "MD041": false,
        "MD029": false,
    },
    "markdownlint.focusMode": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "cSpell.userWords": [
        "amesh",
        "antialiasing",
        "autopep",
        "bigdecimal",
        "binop",
        "BITOR",
        "BITORASSIGN",
        "BITSHIFTL",
        "BITSHIFTR",
        "BITSHIFTRASSIGN",
        "Blacksburg",
        "Blit",
        "btree",
        "byobu",
        "Cascadia",
        "CMAKE",
        "cond",
        "constdefineassign",
        "cpptools",
        "curr",
        "dataclasses",
        "dateutil",
        "DCMAKE",
        "decel",
        "devel",
        "dotfiles",
        "dpkg",
        "eigen",
        "endmacro",
        "Fbuff",
        "gbuff",
        "GLAPIENTRY",
        "GLEW",
        "glsl",
        "glutin",
        "goldmark",
        "gtest",
        "hinj",
        "hlsl",
        "IERP",
        "IFTD",
        "illum",
        "ipynb",
        "Keymap",
        "keyserver",
        "Lbitfield",
        "Lchar",
        "ldconfig",
        "ldict",
        "Lenum",
        "lineinfile",
        "linenos",
        "LPAREN",
        "Lsizei",
        "Luint",
        "mainloop",
        "markdownify",
        "markdownlint",
        "MIPMAP",
        "mipmaps",
        "mkdir",
        "Multisample",
        "mypy",
        "namedtuple",
        "objs",
        "opengl",
        "opname",
        "Parameteri",
        "pbuffer",
        "pinglog",
        "plotjuggler",
        "prec",
        "PREPEND",
        "Programiv",
        "pubconst",
        "pxyz",
        "pycache",
        "pyglet",
        "Pylance",
        "pyperclip",
        "Quickstart",
        "rgba",
        "rgbaf",
        "rgbf",
        "rosbag",
        "roscore",
        "roscpp",
        "rosdep",
        "rosinstall",
        "rosm",
        "rosnode",
        "rosout",
        "rospkg",
        "rospy",
        "rosrun",
        "rosservice",
        "rostopic",
        "RPAREN",
        "rviz",
        "rvizgen",
        "Scarlett",
        "sfml",
        "Shaderiv",
        "Shortcodes",
        "spdlog",
        "sshpass",
        "stof",
        "stoul",
        "struct",
        "structs",
        "submodule",
        "tkinter",
        "tmuxinator",
        "tonemap",
        "torctech",
        "UMINUS",
        "unop",
        "UPLUS",
        "vcstools",
        "Vecs",
        "velodyne",
        "Verts",
        "vlist",
        "voxel",
        "vpntruck",
        "vsicons",
        "vsintellicode",
        "vtools",
        "VTTI",
        "winfo",
        "wstool",
        "xclip"
    ],
    "python.showStartPage": false,
    "explorer.confirmDragAndDrop": false,
    "python.analysis.typeCheckingMode": "basic",
    "cmake.configureOnOpen": false,
    "explorer.confirmDelete": false,
    "explorer.autoReveal": false,
    "files.associations": {
        "*.vsh": "glsl",
        "*.fsh": "glsl",
        "*.vert": "glsl",
        "*.frag": "glsl",
        "*.glsl": "glsl"
    },
    "cSpell.enableFiletypes": [
        "bat",
        "cmake",
        "coffeescript",
        "dockerfile",
        "glsl",
        "hinj",
        "hlsl",
        "ini",
        "jupyter",
        "lua",
        "makefile",
        "properties",
        "shellscript",
        "toml",
        "xml"
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "rust-analyzer.inlayHints.enable": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "diffEditor.ignoreTrimWhitespace": false,
    "redhat.telemetry.enabled": true,
    "python.languageServer": "Pylance",
    "python.analysis.memory.keepLibraryAst": true,
    "python.linting.mypyEnabled": true,
    "docker.showStartPage": false,
    "editor.insertSpaces": true,
    "telemetry.telemetryLevel": "off",
    "git.confirmSync": false,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "git.detectSubmodules": false,
    "git.autoRepositoryDetection": "openEditors",
    "debug.onTaskErrors": "abort",
    "markdownlint.run": "onSave"
}